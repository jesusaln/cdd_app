  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "laravel": "php artisan serve --host=0.0.0.0 --port=8000",
    "start": "concurrently \"npm run laravel\" \"npm run dev\"",
